{
  "env": {
      "browser": true,
      "es6": true,
      "jest": true
  },
  "extends": [
      "airbnb-base"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
      "no-unused-expressions": 0,
      "no-param-reassign": 0,
      "import/no-extraneous-dependencies": 0,
      "spaced-comment": 0
  }
}